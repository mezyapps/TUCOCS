<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:weightSum="5">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:background="#fff"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/gr_voucherno"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:fontFamily="calibri"
            android:textAlignment="textStart"
            android:textColor="#000"
            android:textSize="14sp"
            android:scaleX=".9"
            android:text="Name"
            tools:ignore="RtlCompat">>
        </TextView>
        <TextView android:id="@+id/gr_date"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:fontFamily="calibri"
            android:textAlignment="textEnd"
            android:textColor="#000"
            android:textSize="14sp"
            android:scaleX=".9"
            android:text="PAN NO"
            tools:ignore="RtlCompat">
        </TextView>

    </LinearLayout>
    <LinearLayout
        android:paddingTop="2dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:background="#fff"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/gr_code"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:paddingLeft="2dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:fontFamily="calibri"
            android:textAlignment="textStart"
            android:textColor="#000"
            android:textSize="14sp"
            android:scaleX=".9"
            android:text="ADHAR"
            tools:ignore="RtlCompat">
        </TextView>
        <TextView android:id="@+id/gr_parcelno"
            android:layout_width="0dp"
            android:layout_weight="3.5"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="MOBILE"
            android:fontFamily="calibri"
            android:textAlignment="textEnd"
            android:textColor="#000"
            android:textSize="14sp"
            android:scaleX=".9"
            tools:ignore="RtlCompat">
        </TextView>


    </LinearLayout>
    <LinearLayout
        android:paddingTop="2dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:background="#fff"
        android:layout_height="wrap_content">
        <TextView android:id="@+id/gr_name"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:fontFamily="calibri"
            android:textAlignment="textStart"
            android:textColor="#000"
            android:textSize="14sp"
            android:paddingLeft="2dp"
            android:text="Date"
            android:scaleX=".9"
            tools:ignore="RtlCompat">
        </TextView>
        <TextView
            android:id="@+id/gr_narretion"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:fontFamily="calibri"
            android:text="status"
            android:textAlignment="textEnd"
            android:textColor="#000"
            android:textSize="14sp"
            android:scaleX=".9"
            tools:ignore="RtlCompat">
        </TextView>

    </LinearLayout>
    <LinearLayout
        android:layout_marginBottom="2dp"
        android:paddingTop="2dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:background="#fff"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/gr_remarks"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="calibri"
            android:paddingLeft="12dp"
            android:textColor="#000"
            android:textSize="14sp"
            android:text="Remarks"
            tools:ignore="RtlCompat">
        </TextView>

    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="#E7DEDE"/>

</LinearLayout>
